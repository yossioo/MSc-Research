<?xml version="1.0"?>
<sdf version="1.4">
<model name="%s">
  <pose>0 0 0 0 0 0</pose>
  <static>false</static>
    <link name="%s">
      <gravity>1</gravity>
      <inertial>
        <mass>1.0</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.1</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0.1</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0.1</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.1</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0.1</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
        <mesh>
          <uri>model://simple/%s</uri>          <scale>%f %f %f</scale>
        </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
        <mesh>
          <uri>model://simple/%s</uri>          <scale>%f %f %f</scale>
        </mesh>
        </geometry>
      </visual>
    </link>
  </model>
</sdf>
