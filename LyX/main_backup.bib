@Article{ROA200812709,
  author   = {Máximo A. Roa and Raúl Suárez},
  title    = {Determining Fixturing Points for Complex Objects},
  journal  = {IFAC Proceedings Volumes},
  year     = {2008},
  volume   = {41},
  number   = {2},
  pages    = {12709 - 12714},
  issn     = {1474-6670},
  note     = {17th IFAC World Congress},
  abstract = {This paper presents a method to design fixtures for complex objects in robotized environments. The object is described with a set of points on its surface. First, an initial form-closure fixture is found with an iterative algorithm. The initial fixture is then improved with an iterative approach relying on a geometrical reasoning that efficiently looks for a locally optimum fixture; the quality of the fixture is measured considering the largest perturbation wrench that the fixture can resist, with independence of its direction. Once a locally optimum fixture has been reached, independent contact regions are computed to provide robustness in front of the locator positioning errors or to allow variations in the locators position. The proposed approach can also be applied to compute fixtures when one or several locators are fixed beforehand. The procedure has been implemented and application examples are included in the paper.},
  doi      = {https://doi.org/10.3182/20080706-5-KR-1001.02150},
  keywords = {fixture design, frictionless grasps, independent contact regions},
  url      = {http://www.sciencedirect.com/science/article/pii/S1474667016410165},
}

