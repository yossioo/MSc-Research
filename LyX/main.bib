@Article{Bezak2014,
  author   = {Pavol Bezak and Pavol Bozek and Yuri Nikitin},
  title    = {Advanced Robotic Grasping System Using Deep Learning},
  journal  = {Procedia Engineering},
  year     = {2014},
  volume   = {96},
